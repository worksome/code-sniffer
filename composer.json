{
    "name": "worksome/code-sniffer",
    "description": "Worksome's preferences and custom sniffers for phpcs",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Nybroe",
            "email": "oliver@worksome.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Worksome\\WorksomeSniff\\": "src/WorksomeSniff"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worksome\\WorksomeSniff\\Tests\\": "tests"
        }
    },
    "require": {
        "PHP": "^8.0",
        "squizlabs/php_codesniffer": "^3.6.1",
        "slevomat/coding-standard": "^8.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "thecodingmachine/safe": "^2.0",
        "illuminate/support": "^8.78 || ^9.0",
        "illuminate/collections": "^8.78 || ^9.0",
        "illuminate/contracts": "^8.78 || ^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "symfony/var-dumper": "^5.3"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
